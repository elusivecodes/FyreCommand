{
    "name": "fyre/command",
    "description": "A command library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "elusivecodes",
            "email": "elusivecodes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fyre\\Command\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "fyre/console": "^5.0",
        "fyre/container": "^2.0",
        "fyre/event": "^5.0",
        "fyre/inflector": "^4.0",
        "fyre/loader": "^4.0",
        "fyre/macro": "^2.0",
        "fyre/typeparser": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.59",
        "fyre/php-cs-fixer-config": "^1.0",
        "phpunit/phpunit": "^12"
    },
    "scripts": {
        "cs": "php-cs-fixer fix --ansi --verbose --dry-run --diff",
        "cs-fix": "php-cs-fixer fix --ansi --verbose --diff",
        "test": "phpunit tests"
    }
}